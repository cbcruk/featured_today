---
import type { AppEntry } from '@content/types'
import { getCollection } from 'astro:content'

type Props = Pick<AppEntry, 'id'>

const { id } = Astro.props
const appCategory = await getCollection(
  'appCategories',
  (appCategory) => appCategory.data.app_id === id
)
